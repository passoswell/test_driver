{
  "version": 4,
  "configurePresets": [
    {
      "name": "pipico_debug",
      "displayName": "pipico debug",
      "description": "",
      "generator": "Ninja Multi-Config",
      "binaryDir": "${sourceDir}/build/${presetName}",
      "cacheVariables": {
        "CMAKE_INSTALL_PREFIX": "${sourceDir}/out/install/${presetName}",
        "CMAKE_C_COMPILER": "/usr/bin/arm-none-eabi-gcc",
        "CMAKE_BUILD_TYPE": "Debug",
        "PICO_SDK_FETCH_FROM_GIT": "ON",
        "PICO_SDK_FETCH_FROM_GIT_PATH": "~/pipico",
        "PIPICO_VERSION": "1.5.1",
        "USE_PIPICO": "1",
        "BUILD_ALL_EXAMPLES": "1"
      }
    },
    {
      "name": "pipico_release",
      "inherits": "pipico_debug",
      "displayName": "pipico release",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "release"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "pipico_debug",
      "configurePreset": "pipico_debug"
    },
    {
      "name": "pipico_release",
      "configurePreset": "pipico_release"
    }
  ]
}
